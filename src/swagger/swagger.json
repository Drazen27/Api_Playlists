      "/api/listas/getAllLists": {
        "get": {
          "summary": "Obtiene todas las listas de reproducción.",
          "description": "Retorna todas las listas de reproducción almacenadas en la base de datos.",
          "responses": {
            "200": {
              "description": "Lista de reproducción obtenida correctamente.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "string",
                          "description": "ID único de la lista de reproducción."
                        },
                        "id_usuario": {
                          "type": "string",
                          "description": "ID del usuario propietario de la lista de reproducción."
                        },
                        "nombre": {
                          "type": "string",
                          "description": "Nombre de la lista de reproducción."
                        },
                        "canciones": {
                          "type": "array",
                          "description": "Lista de canciones incluidas en la lista de reproducción.",
                          "items": {
                            "type": "object",
                            "properties": {
                              "id": {
                                "type": "string",
                                "description": "ID único de la canción."
                              },
                              "nombre": {
                                "type": "string",
                                "description": "Nombre de la canción."
                              },
                              "imagen": {
                                "type": "string",
                                "description": "URL de la imagen de la canción."
                              }
                            }
                          },
                          "example": [
                            {
                              "id": "ni5o2435boidfadsf",
                              "nombre": "Canción 1",
                              "imagen": "https://example.com/imagen1.jpg"
                            },
                            {
                              "id": "432oi5jn2l3k4n5on",
                              "nombre": "Canción 2",
                              "imagen": "https://example.com/imagen2.jpg"
                            }
                          ]
                        }
                      }
                    }
                  }
                }
              }
            },
            "404": {
              "description": "No se encontraron listas de reproducción en la base de datos."
            },
            "500": {
              "description": "Error al obtener las listas de reproducción desde la base de datos."
            }
          }
        }
      },
